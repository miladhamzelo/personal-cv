{
  "name": "personal-cv-ssr",
  "version": "1.0.0",
  "description": "shane arthur personal resume in react with SSR",
  "main": "index.js",
  "scripts": {
    "build": "webpack --config webpack.server.js -p && webpack --config webpack.client.js -p",
    "start": "node build/bundle.js",
    "dev": "npm-run-all --parallel dev:*",
    "dev:server": "nodemon --watch build --exec \"node build/bundle.js\"",
    "dev:build-server": "webpack --config webpack.server.js --watch",
    "dev:build-client": "webpack --config webpack.client.js --watch"
  },

  
  "engines": {
    "node": ">=8.9.0",
    "npm": ">=5.5.1"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "axios": "0.16.2",
    "babel-cli": "6.26.0",
    "babel-core": "6.26.0",
    "babel-loader": "7.1.2",
    "babel-preset-env": "1.6.0",
    "babel-preset-es2015": "6.24.1",
    "babel-preset-es2017": "6.24.1",
    "babel-preset-react": "6.24.1",
    "babel-preset-stage-0": "6.24.1",
    "bluebird": "^3.5.1",
    "compression": "1.7.0",
    "concurrently": "3.5.0",
    "css-loader": "^0.28.7",
    "express": "4.15.4",
    "express-http-proxy": "1.0.6",
    "extract-text-webpack-plugin": "^3.0.2",
    "file-loader": "^1.1.6",
    "lodash": "4.17.4",
    "material-ui": "^0.20.0",
    "node-sass": "^4.7.2",
    "nodemon": "1.12.0",
    "npm-run-all": "4.1.1",
    "react": "16.0.0",
    "react-dom": "16.0.0",
    "react-google-maps": "^9.4.3",
    "react-helmet": "5.2.0",
    "react-redux": "5.0.6",
    "react-router-config": "1.0.0-beta.4",
    "react-router-dom": "4.2.2",
    "redux": "3.7.2",
    "redux-thunk": "2.2.0",
    "sass-loader": "^6.0.6",
    "serialize-javascript": "1.4.0",
    "url-loader": "^0.6.2",
    "webpack": "3.5.6",
    "webpack-dev-server": "2.8.2",
    "webpack-isomorphic-tools": "^3.0.5",
    "webpack-merge": "4.1.0",
    "webpack-node-externals": "1.6.0"
  }
